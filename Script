#include <vector>
#include <algorithm>

const int GRID_SIZE = 10;
const int NUM_WORDS = 5;

char grid[GRID_SIZE][GRID_SIZE];
std::vector<std::string> words;

void initGrid() {
    for (int i = 0; i < GRID_SIZE; i++) {
        for (int j = 0; j < GRID_SIZE; j++) {
            grid[i][j] = '-';
        }
    }
}

void initWords() {
    words.push_back("c++");
